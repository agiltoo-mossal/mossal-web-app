{
  "name": "mossal-web-app",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --port 4500",
    "build": "ng build --configuration production",
    "build:dev": "ng build --configuration development",
    "watch": "ng build --watch --configuration local",
    "test": "ng test",
    "dev": "run-p start graphql-gen:watch",
    "graphql-gen": "graphql-codegen  --config ./src/app/graphql/codegen.ts --require dotenv/config",
    "graphql-gen:watch": "graphql-codegen  --config ./src/app/graphql/codegen.ts --require dotenv/config --watch"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^16.1.0",
    "@angular/cdk": "^16.1.2",
    "@angular/common": "^16.1.0",
    "@angular/compiler": "^16.1.0",
    "@angular/core": "^16.1.0",
    "@angular/flex-layout": "^15.0.0-beta.42",
    "@angular/forms": "^16.1.0",
    "@angular/material": "^16.2.9",
    "@angular/platform-browser": "^16.1.0",
    "@angular/platform-browser-dynamic": "^16.1.0",
    "@angular/router": "^16.1.0",
    "@apollo/client": "^3.0.0",
    "@kolkov/angular-editor": "^3.0.0-beta.0",
    "@ng-bootstrap/ng-bootstrap": "^15.1.2",
    "@ngx-translate/core": "^15.0.0",
    "@ngx-translate/http-loader": "^8.0.0",
    "@popperjs/core": "^2.11.6",
    "apollo-angular": "^5.0.0",
    "bootstrap": "^5.2.3",
    "graphql": "^16",
    "install": "^0.13.0",
    "jquery": "^3.7.1",
    "ng-terminal": "^6.0.0-beta",
    "ng2-google-charts": "^7.0.0",
    "ng2-pdf-viewer": "^10.0.0",
    "ngx-captcha": "^13.0.0",
    "ngx-extended-pdf-viewer": "^18.1.5",
    "npm": "^9.8.1",
    "rxjs": "~7.8.0",
    "slick-carousel": "^1.8.1",
    "tslib": "^2.3.0",
    "xterm": "^5.2.1",
    "zone.js": "~0.13.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^16.1.1",
    "@angular/cli": "~16.1.1",
    "@angular/compiler-cli": "^16.1.0",
    "@angular/localize": "^16.1.0",
    "@graphql-codegen/cli": "^4.0.1",
    "@graphql-codegen/typescript": "^4.0.1",
    "@graphql-codegen/typescript-apollo-angular": "^3.5.6",
    "@graphql-codegen/typescript-operations": "^4.0.1",
    "@types/jasmine": "~4.3.0",
    "jasmine-core": "~4.6.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "npm-run-all": "^4.1.5",
    "typescript": "~5.1.3"
  }
}